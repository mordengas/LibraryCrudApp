<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.projektzaliczeniowy.projektzaliczeniowy.SceneController">
    <children>
        <AnchorPane fx:id="glowny" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0"
                    VBox.vgrow="ALWAYS">
            <children>
                <Text fx:id="title_a" layoutX="173.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="Co chcesz zrobić?" textAlignment="CENTER" wrappingWidth="292.796875">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Text>
                <Button fx:id="wstecz" layoutX="230.0" layoutY="351.0" mnemonicParsing="false"
                        onAction="#switchToProjekt" prefHeight="35.0" prefWidth="181.0" text="Wstecz"/>
                <Button fx:id="zarzWypozyczeniami" layoutX="229.0" layoutY="242.0" mnemonicParsing="false"
                        onAction="#switchToZarzadzanieWypozyczeniami" prefHeight="51.0" prefWidth="181.0"
                        text="Zarządzanie Wypożyczeniami"/>
                <Button fx:id="zarzKsiazkami" layoutX="230.0" layoutY="105.0" mnemonicParsing="false"
                        onAction="#switchToZarzadzanieKsiazkami" prefHeight="51.0" prefWidth="181.0"
                        text="Zarządzanie Książkami"/>
                <Button fx:id="zarzKlientami" layoutX="229.0" layoutY="175.0" mnemonicParsing="false"
                        onAction="#switchToZarzadzanieKlientami" prefHeight="51.0" prefWidth="181.0"
                        text="Zarządzanie Klientami"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
