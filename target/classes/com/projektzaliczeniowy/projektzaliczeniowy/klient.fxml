<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.projektzaliczeniowy.projektzaliczeniowy.SceneController">
    <children>
        <AnchorPane fx:id="glowny" maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0"
                    VBox.vgrow="ALWAYS">
            <children>
                <Text fx:id="title_k" layoutX="173.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="Co chcesz zrobić?" textAlignment="CENTER" wrappingWidth="292.796875">
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Text>
                <Button fx:id="wstecz" layoutX="230.0" layoutY="351.0" mnemonicParsing="false"
                        onAction="#switchToProjekt" prefHeight="35.0" prefWidth="181.0" text="Wstecz"/>
                <Button fx:id="wypozycz" layoutX="230.0" layoutY="138.0" mnemonicParsing="false"
                        onAction="#switchToWypozyczenie" prefHeight="51.0" prefWidth="181.0" text="Wypożycz książkę"/>
                <Button fx:id="zwroc" layoutX="230.0" layoutY="226.0" mnemonicParsing="false" onAction="#switchToZwrot"
                        prefHeight="51.0" prefWidth="181.0" text="Zwróć książkę"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
